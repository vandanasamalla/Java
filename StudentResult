package vandana;

public class StudentResult {
	public static void main (String args[]) {
		int []marks= {43,56,73,65,59,76};
		boolean b=true;
		int i=0,sum=0;
		double percentage;
		for(;i<marks.length;) {
		if(marks[i]<=35)
			b=false;
		i++;
		
		}
		if(b) {
			for(i=0;i<marks.length;i++) {
				sum+=marks[i];
				
			}
			percentage=sum/6;
			System.out.println("studenr result:passed;\nTotal="+sum+"percentage="+percentage);
		}else
			System.out.println("student Failed");
			
	}

}
