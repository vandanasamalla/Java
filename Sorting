

package vandana;

public class Sorting {
	public static void main(String args[]) {
	int []arr= {34,56,23,56,22,95,24,65,49};	
     int n=arr.length,i,j,temp;
     for(i=0;i<n;i++) {
    	 for(j=1;j<n;j++) 
    	 {
    		 if(arr[j-1]>arr[j]) {
    			 temp=arr[j];
    			 arr[j]=arr[j-1];
    			 arr[j-1]=temp;
    					 
    		 }
    	 }
    		  

	}
     System.out.print("Sorted array :" );

     for(i=0;i<arr.length;i++)
    	 System.out.print(+arr[i]+"  ");
	}
} 
